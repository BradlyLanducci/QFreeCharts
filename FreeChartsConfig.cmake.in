@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

# Export the targets of the library
include("${CMAKE_CURRENT_LIST_DIR}/FreeChartsTargets.cmake")

set(FREECHARTS_INCLUDE_DIRS "@CMAKE_INSTALL_PREFIX@/include")

# Optionally, set variables to help find the library in other projects
set(FREECHARTS_LIBRARIES FreeCharts)

